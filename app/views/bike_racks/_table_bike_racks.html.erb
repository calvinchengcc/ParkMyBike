<%= render 'search_box' %>
<div id="bike-table">
    <table class="table table-hover table-condensed">
        <% @bike_racks.each do |bike_rack| %>
          <tr>
            <td><%= bike_rack.street_number %>, <%= bike_rack.street_name %></td>
            <td><%= bike_rack.number_of_racks %> bike racks</td>
          </tr>
        <% end %>
    </table>
</div>

