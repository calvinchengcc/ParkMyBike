<div class="btn-group btn-group-justified" role="group" aria-label="...">
    <%= link_to 'Update', full_update_path, class: 'btn btn-primary disabled' %>
    <%= link_to 'Clear', full_clear_path, class: 'btn btn-primary' %>
</div>
<%= render 'search_box' %>
<div id="bike-table">
    <table class="table table-hover table-condensed">
        <% @bike_racks.each do |bike_rack| %>
          <tr>
            <td><%= bike_rack.street_number %>, <%= bike_rack.street_name %></td>
            <td><%= bike_rack.number_of_racks %> bike racks</td>
          </tr>
        <% end %>
    </table>
</div>

