<%= form_tag bike_racks_path, method: 'get', class: 'navbar-form', role: 'search' do %>
  <div class="input-group">
    <%= text_field_tag :search, params[:search], size: 150, placeholder: "Search", class: "form-control"%>
  <div class="input-group-btn">
    <%= button_tag(type: "submit", class: "btn btn-default") do %>
      <i class="glyphicon glyphicon-search"></i>
    <% end %>
  </div></div>
<% end %>
