  <div id="fb-root"></div>
  <script>
  window.fbAsyncInit = function() {
  FB.init({appId: '706274382806077', status: true, cookie: true,
  xfbml: true});
  };
  (function() {
  var e = document.createElement('script'); e.async = true;
  e.src = document.location.protocol +
  '//connect.facebook.net/en_US/all.js';
  document.getElementById('fb-root').appendChild(e);
  }());
  </script>

  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js" type="text/javascript"></script>


  <script type="text/javascript">
  $(document).ready(function(){
  $('#share_button').click(function(e){
  e.preventDefault();
  FB.ui(
  {
  method: 'feed',
  name: 'PikeMyBike',
  link: 'http://screaming-tortoises.herokuapp.com',
  picture: 'http://i3.getreading.co.uk/incoming/article6191561.ece/ALTERNATES/s1200/Bike-rack-1.jpg',
  caption: 'Looking for a place to park your bike?',
  description: "What you can do here? 1.See a list of bike racks 2.See a map of bike racks 3.Register to rate 4.Share on Facebook",
  message: ""
  });
  });
  });
  </script>

  <div class="container">
    <div class="page-header">
      <h1><%=current_user.username%> Profile 
        <% if current_userF%> <%= image_tag current_userF.myImage, :size => "64x64" %><%end%></h1>
    </div>
    <p class="lead">Username: <%=current_user.username%></p>
    <p class="lead">Email: <%=current_user.email%></p>
    <% if current_userF %>
    <p class="lead">Number of friends: <%=current_userF.friends%></p>
    <p>Signed in as <strong><%= current_userF.name %></strong>!
      <%= link_to "Sign out", signout_path, id: "sign_out" %></p>
    <% else %>
      <%= link_to "Sign in with Facebook", "/auth/facebook", id: "sign_in" %>
    <% end %>
    
  <p><img src = "http://www.shar9.com/styles/default/xenforo/facebook_share4.png" id = "share_button"><%= social_share_button_tag("Test_Title", :popup => "true", :url => "http://screaming-tortoises.herokuapp.com") %></p>
  </div>


