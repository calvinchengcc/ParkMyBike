<!DOCTYPE html>
<html>
<head>
  <title>ParkMyBike</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link rel="shortcut icon" href="favicon.ico" />
</head>
<body>
  <header>
    <div class="navbar navbar-fixed-top navbar-inverse">
       <div class="navbar-inner">
          <a class="navbar-brand" href="/">
            <img class = "logo logo_default " src="/media/logowhite.png" height="40" width="40">
            <img class = "logo logo_hover"src="/media/logoblue.png" height="40" width="40">
          </a>
          <ul class="nav navbar-nav navbar-left">
            <li><%= link_to "Home", href="/"%></li>
            <li><%= link_to "Our Team", href="/team"%></li>
            <li><%= link_to "Bike Racks", bike_racks_path%></li>
            <li><%= link_to "Manage Data", internal_path%></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <%= render "users/navbar" %>
          </ul>
       </div>
    </div>
  </header>
  <!-- flash notice -->
  <%= render "layouts/flash" %>
<%= yield %>

</body>
</html>
